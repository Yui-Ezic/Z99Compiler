{
    "name": "yui-ezic/z99-compiler",
    "description": "Compiler for Z99 language",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Misha Zuev",
            "email": "mishan221199@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "antlr/antlr4-php-runtime": "dev-master",
        "symfony/console": "^5.1@dev",
        "graphp/graphviz": "^0.2.2",
        "ext-ctype": "*",
        "ext-readline": "*"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.2@dev"
    },
    "autoload": {
        "psr-4": {
            "Generated\\": "Generated/",
            "Z99Compiler\\": "src/",
            "SemanticAnalyzer\\": "SemanticAnalyzer/",
            "Z99Interpreter\\": "Interpreter/",
            "Z99Parser\\": "Parser/",
            "Z99Lexer\\": "Lexer/",
            "Commands\\": "Commands/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
